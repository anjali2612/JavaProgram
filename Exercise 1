4. Fibonacci Series 

import java.math.*;

public class App {

    public static void main(String[] args) {
        int n1 = 0;
        int n2 = 1;
        System.out.println(n1 + "\n" + n2);
        int n3 = 1, count = 10;
        for (int i = 0; i <= count; i++) {
            n3 = n1 + n2;
            System.out.println(" " + n3);
            n1 = n2;
            n2 = n3;

        }
    }
}

